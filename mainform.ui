}

QToolButton {
	border:0;
}

QToolButton#closeBtn {
	background-image: url(:/images/close.png);
}

QToolButton#closeBtn:hover {
	background-image: url(:/images/close_h.png);
}
QToolButton#closeBtn:pressed {
	background-image: url(:/images/close_p.png);
}

QToolButton#minBtn {
	background-image: url(:/images/min.png);
}

QToolButton#minBtn:hover {
	background-image: url(:/images/min_h.png);
}
QToolButton#minBtn:pressed {
	background-image: url(:/images/min_p.png);
}

QLabel {
	color:gray;
	font:12pt;
}

QToolBox  {
    padding: 5px;
    border: 10px transparent black;
	min-height:30px; 
}

QToolBox::tab {
    color:white;
font:10pt;
	background-color: rgb(0, 0, 255);
    border: 1px solid #76797C;
    border-bottom: 1px transparent #31363b;
}

QToolBox::tab:selected { /* italicize selected tabs */
    font: italic;
   	
	background-color: rgb(0, 0, 255);
    border-color: #3daee9;
 }



QToolButton::menu-button {
         border: 2px solid gray;
        border-top-right-radius: 6px;
         border-bottom-right-radius: 6px;
         width: 16px;
}
QToolButton::menu-arrow:open {
         top: 1px; left: 1px; /* shift it a bit */
}
QTableView {
	selection-background-color:rgb(131,168,238)
}
QLineEdit {
	color:rgb(127,0,63);
	border:2px groove gray;
	border-radius:6px;
	selection-color: white;
    selection-background-color: rgb(191, 31, 127);
}
QPushButton {
    border: 2px solid #8f8f91;
    border-radius: 6px;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                      stop: 0 #f6f7fa, stop: 1 #dadbde);
	min-height:20px;
	min-width:50px;
}

QPushButton:hover {
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                      stop: 0 #dadbde, stop: 1 #f6f7fa);
}

QPushButton:pressed {
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                      stop: 0 #dadbde, stop: 1 #f6f7fa);
	
border-style: inset;
}

QPushButton:flat {
    border: none; /* no border for a flat push button */
}

QPushButton:default {
    border-color: navy; /* make the default button prominent */
}</string>
